
.app-modal-body {
  background-color: $white;
  z-index: 999;
  width: 400px;
}

.app-modal-left {
  height: 70%;
  width: 40%;
  float: left;
}
.app-modal-left > li {
  height: 45px;
  text-align: left;
  padding-left: 20px;
  font-size: 14px;
}
.app-modal-right {
  height: 70%;
  width: 40%;
  float: right;
  text-align: center;
}
.img-show{
  width: 50%;
  float: left;
  text-align: center;
  padding: 5px;
  margin-top: 50px;
}
.li-span {
  text-align: center;
  width: 25px;
  height: 25px;
  background-color: #008ee5;
  border-radius: 50%;
  font-size: 14px;
  display: inline-block;
  line-height: 25px;
  margin-right: 20px
}

.aColor {
  color: #008ee5;
}
.app-modal-Icon {
  position: absolute;
  top: 15px;
  right: 16px;
  font-size: 16px;
  cursor: pointer;
}

.loadDownPosition {
  position: absolute;
  top: 40%;
  left: 50%;
  width: 800px;
  transform: translate(-50%, -50%);
  height: 500px;
  border: none;
  color: $white
}

.appCardBodyWinMoney {
  background-color: $tab-select;
  padding: 20px;
  z-index: 999;
  color: $white;
}

.userInfoButton {
  display: inline-block;
  width: 80px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  background-color: $tab-select;
  color: $white;
  border-radius: 20px;
  float: left;
  border: none;
  position: absolute;
  bottom: 0;
  left: calc(50% - 24px);
  transform: translateX(-50%);
}
.userInfoText {
  height: 35px;
  line-height: 35px;
  margin-bottom: 10px;
}
  .reload-action:hover{
    color :$orange
  }
.addCardText {
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size: 14px;
  font-weight: 600;
  color: $white;
  height: 45px;
  line-height: 45px;
}
.label-font {
  font-size: 16px;
  font-weight: bold;
  color: $tab-select
}
.disableChooseBtn{
  cursor:not-allowed!important;
  background:#CCC!important;
}
.accountBtn{
  display: inline-block;
  padding: 0 15px;
  border:1px solid $tab-select;
  height: 30px;
  border-radius: 15px;
  color:$tab-select;
  line-height: 26px;
  font-weight: 600;
  font-size: 16px;
  margin-left: 5px;
  cursor: pointer;
}
.scroll::-webkit-scrollbar { /*滚动条整体样式*/
  width: 4px; /*高宽分别对应横竖滚动条的尺寸*/
  height: 4px;
}

.scroll::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
  border-radius: 5px;
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  background: rgba(0, 0, 0, 0.2);
}

.scroll::-webkit-scrollbar-track { /*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 0;
  background: rgba(0, 0, 0, 0.1);
}
.titleClass {
  font-size: 25px !important;
  margin-top: 10px;
  color: $tab-select;
  margin-bottom :5px
}
.img {
  width: 100%;
  max-height: 260px;
  overflow: hidden;
}
.activityLogoPage {
  width: 100%;
  height: 100%;
}
.fontPromotion {
  display: inline-block;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  height: 90px;
  color: #000;
  font-size: 14px;
  letter-spacing: 1px;
  line-height: 30px;
  margin-bottom 10px
}
.btnPromotion {
  width: 160px;
  height: 38px;
  line-height: 38px;
  text-align: center;
  display: inline-block;
  background: #3cafee;
  margin: 0 auto;
  border: none
  color: #fff;
}
.liPromotion {
  line-height: 30px;
  color: #000;
}
.positionBodyModal {
  height: 96%;
  width: 100%;
  z-index: 1000;
  background: rgb(255, 255, 255);
  text-align: center;
  padding: 50px;
  padding-top 16px;
}
.aTitle{
    font-size: 19px;
    margin-top: 10px;
    text-align: center;
    font-weight: 700;
    margin-bottom: 5px;
  }
.pit{
  height: 100%;
  width: 100%;

}
.close-modal {
  position: absolute;
  top: 20px;
  right: 20px;
  font-size: 20px;
}
.activityTitle {
  height: 50px;
}
.playResultSign {
  display: inline-block;
  width: 80px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  background-color: #121435;
  color: #fff;
  border-radius: 20px;
  margin: 10px auto;
  border: none;
  box-shadow: rgb(170, 170, 170) 0px 3px 8px, rgb(255, 255, 255) 0px 2px 3px inset;
}
.lottery-code-order{
  width :33%;
  display :inline-block;
}


  //走势图

#ChartContent {
  box-sizing: content-box;
  -moz-box-sizing: content-box; /* Firefox */
  -webkit-box-sizing: content-box;
  background-color: #FFFFFF;
}

#ChartContent .trendChartTitle {
  text-align: left;
  line-height: 28px;
  background: #0D2F55;
  font-size: 14px;
  color: #fff;
  padding-left: 20px;
  margin-bottom: 0;
}

#ChartContent .trenChartFeature, .toolBar {
  background: #6FE6FB;
  padding: 10px 0;
}

#ChartContent .trendChart a {
  font-size: 13px;
  color: #999;
}

#ChartContent .trenChartFeature a:hover {
  text-decoration: underline;
  cursor: pointer
}

#ChartContent .trenChartFeature > span, .toolBar > span {
  margin-left: 20px;
  line-height: 16px;
}

#ChartContent .trenChartFontActive {
  color: #555 !important;
}

#ChartContent .trendChartTitle th {
  font-weight: normal;
  background: #e8eced;
}

#ChartContent .chartTable {
  border-collapse: collapse;
}

#ChartContent .chartTable th {
  font-weight: normal;
  background: #e8eced;
}

#ChartContent th, #ChartContent td {
  text-align: center;
  height: 30px;
  width: 17px;
  padding: 5px;
  text-align: center;
  white-space: nowrap;
  border: 1px solid #fff !important;
}

#ChartContent .chartlost {
  display: inline-block;
  width: 20px;
}

#ChartContent .borderRight {
  border-right: 1px solid red;
}

#ChartContent .table {
  border-right: none;
  border-left: none;
  border-bottom: none;
  position: relative;
  bottom: 0px;
}

#ChartContent .active {
  display: inline-block;
  width: 16px;
  height: 16px;
  line-height: 16px;
  border: 1px solid red;
  border-radius: 50%;
  background-color: red;
  color: #FFFFFF;
}

#ChartContent td {
  border: '1px solid #fff';
  background-color: '#E8ECED';

}

#ChartContent .issue {
  display: flex;
  justify-content: flex-start;
}

#ChartContent .singleActive {
  background-color: #FE8098;
  color: #555;
}

#ChartContent .ActiveBlue {
  background-color: #6FE6FB;
  color: #555;
}

#ChartContent .ActiveYell {
  background-color: #FEF8AC;
  color: #555;
}

#ChartContent.ActiveRed {
  background-color: red;
  color: #555;
}

#ChartContent .display {
  display: none;
}

#ChartContent .fiveStartWidth {
  width: 40px;
}

#ChartContent .ThreeStartWidth {
  width: 55px;
}

#ChartContent .TwoStartContentTd {
  width: 170;
}

#ChartContent .subline {
  border-bottom: 2px solid red;
}

#ChartContent .ActiveBlack {
  display: inline-block;
  width: 16px;
  height: 16px;
  line-height: 16px;
  border-radius: 50%;
  background-color: black;
  color: #fff;
}

#ChartContent .ActivePurple {
  background-color: #A349A4;
  display: inline-block;
  width: 16px;
  height: 16px;
  line-height: 16px;
  border-radius: 50%;
  color: #FFFFFF;
}

#ChartContent .Display {
  display: none;
}

#ChartContent .issueContent {
  width: 100%;
  height: 1000px;
}

